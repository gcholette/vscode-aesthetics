      "settings": {
        "foreground": "#E5E9F0"
      }
    },
    {
      "scope": ["source.scss keyword.control"],
      "settings": {
        "foreground": "#30BDFF"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#357AFF"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#8233FF"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#30BDFF"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#DBEEFF"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#DBEEFF"
      }
    },
