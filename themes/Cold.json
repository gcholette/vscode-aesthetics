      "settings": {
        "foreground": "#DBEEFF"
      }
    },
    {
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#5960B4"
      }
    },
    {
      "scope": ["meta.separator"],
      "settings": {
        "foreground": "#A7A8AF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#5960B4"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#30BDFF"
      }
