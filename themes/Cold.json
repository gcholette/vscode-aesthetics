    {
      "scope": ["support.other.variable", "string.other.link", "markup.table"],
      "settings": {
        "foreground": "#E5E9F0"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
